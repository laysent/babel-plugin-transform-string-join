{
  "name": "babel-plugin-transform-string-join",
  "version": "1.0.0",
  "description": "Babel plugin that join strings and template strings in compilation when possible",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --presets es2015 --source-maps",
    "prepublish": "npm run build",
    "test": "jasmine JASMINE_CONFIG_PATH=./jasmine.json ./test/index.js"
  },
  "keywords": [
    "string",
    "concat",
    "join",
    "babel-plugin"
  ],
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "jasmine": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laysent/babel-plugin-transform-string-join.git"
  },
  "author": {
    "name": "LaySent",
